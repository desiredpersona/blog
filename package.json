{
  "name": "nostalgia",
  "version": "0.2.0",
  "description": "A minimal Eleventy starter created from first principles thinking",
  "scripts": {
    "build": "ELEVENTY_ENV=production npx eleventy",
    "build:dev": "ELEVENTY_ENV=development npx eleventy",
    "serve": "ELEVENTY_ENV=development npx eleventy --serve --watch",
    "debug": "DEBUG=Eleventy* npx eleventy",
    "debug:performance": "DEBUG=Eleventy:Benchmark* npx eleventy"
  },
  "author": {
    "name": "Desired Persona",
    "url": "https://desiredpersona.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/desiredpersona/nostalgia.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-upgrade-help": "^2.0.5",
    "clean-css": "^5.3.2",
    "html-minifier": "^4.0.0",
    "markdown-it": "^13.0.1",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-footnote": "^3.0.3",
    "path": "^0.12.7"
  }
}
